{{- include header }}
<div class="content row">
  <div class="col-md-1"></div>
  <div class="index-container col-md-7">
    <div class="index-area">
    {{ posts.forEach(function(post) {  }}
      <div class="panel panel-default list-area">
        <div class="panel-heading entry-title"><a href="/p/{{=post._id}}">{{=post.title}}</a></div>
        <div class="panel-body">
          <div class="post-status">POST on:&nbsp<span class="time-sty">{{=post.time.minutes}}</span>&nbsp;Read:{{=post.pv}}</div>
          <p class="article-content">{{-post.post}}</p>
        </div>
      </div>
    {{ }) }}
    </div>
  </div>
  {{- include widgets }}
</div>
{{- include footer }}
</div>
</body>
<script src='/javascripts/jquery-1.10.2.min.js'></script>
<script src='/javascripts/bootstrap.min.js'></script>
<script src='/javascripts/main.js'></script>
