{{ include header }}
<div class="register-container row">
<div class="col-md-4"></div>
<form class="form-horizontal register-area col-md-4" role="form">
  <fieldset>
    <div class="form-group">
      <label class="col-sm-2 control-label">username</label>
      <div class="col-sm-8">
        <input type="text" class="register-username form-control" name="username" placeholder="username">
      </div>
    </div>
    <div class="form-group">
      <label class="col-sm-2 control-label">email</label>
      <div class="col-sm-8">
        <input type="text" class="register-email form-control" name="email" placeholder="email">
      </div>
      </div>
      <div class="form-group">
        <label class="col-sm-2 control-label">password</label>
        <div class="col-sm-8">
          <input type="password" class="register-password form-control column" name="password" placeholder="password">
        </div>
      </div>
      <div class="error-tip"></div>
      <div class="register-btn btn btn-primary">Register</div>
    </div>
    </fieldset>
  </form>
  <div class="col-md-4"></div>
</div>
{{- include footer}}
